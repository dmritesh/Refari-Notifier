# Database Connection
DATABASE_URL="postgresql://postgres:password@postgres:5432/hubstaff_automation?schema=public"

# Application Config
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# Master Key for Encryption (Must be 32 characters for AES-256)
MASTER_ENCRYPTION_KEY="replace_with_32_characters_secret"

# Hubstaff OAuth
HUBSTAFF_CLIENT_ID="your_client_id"
HUBSTAFF_CLIENT_SECRET="your_client_secret"
HUBSTAFF_REDIRECT_URI="http://localhost:3000/auth/hubstaff/callback"
ADMIN_PASSWORD=change_me_in_production
